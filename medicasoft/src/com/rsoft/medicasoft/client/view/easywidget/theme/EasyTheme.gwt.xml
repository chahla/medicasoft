		<when-type-is
			class="com.sencha.gxt.widget.core.client.PlainTabPanel.PlainTabPanelAppearance" />
	</replace-with>

	<!-- Color Palette -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyColorPaletteAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.ColorPaletteCell.ColorPaletteAppearance" />
	</replace-with>

	<!-- Slider -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasySliderHorizontalAppearance">
		<any>
			<when-type-is
				class="com.sencha.gxt.cell.core.client.SliderCell.SliderAppearance" />
			<when-type-is
				class="com.sencha.gxt.cell.core.client.SliderCell.HorizontalSliderAppearance" />
		</any>
	</replace-with>
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasySliderVerticalAppearance">
		<when-type-is
			class="com.sencha.gxt.cell.core.client.SliderCell.VerticalSliderAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyBorderLayoutAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutAppearance" />
	</replace-with>

	<!-- Resizable -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyResizableAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.Resizable.ResizableAppearance" />
	</replace-with>

	<!-- Status proxy -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyStatusProxyAppearance">
		<when-type-is
			class="com.sencha.gxt.dnd.core.client.StatusProxy.StatusProxyAppearance" />
	</replace-with>
	<!-- Paging tool bar -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyPagingToolBarAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.toolbar.PagingToolBar.PagingToolBarAppearance" />
	</replace-with>
