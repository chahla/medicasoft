			class="com.sencha.gxt.widget.core.client.menu.MenuBarItem.MenuBarItemAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyDatePickerAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.DatePicker.DatePickerAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyFieldSetAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.form.FieldSet.FieldSetAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyTreeAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.tree.Tree.TreeAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyContentPanelAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.ContentPanel.ContentPanelAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyFramedPanelAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.FramedPanel.FramedPanelAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyToolBarAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.toolbar.ToolBar.ToolBarAppearance" />
	</replace-with>


	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasySeparatorToolItemAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.toolbar.SeparatorToolItem.SeparatorToolItemAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyTools">
		<when-type-is class="com.sencha.gxt.widget.core.client.button.Tools" />
	</replace-with>
	<!-- Menu -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyMenuAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.Menu.MenuAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyItemAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.Item.ItemAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyMenuItemAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.MenuItem.MenuItemAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyHeaderMenuItemAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.HeaderMenuItem.HeaderMenuItemAppearance" />
	</replace-with>


	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasySeparatorMenuItemAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.SeparatorMenuItem.SeparatorMenuItemAppearance" />
	</replace-with>


	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyCheckMenuItemAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.CheckMenuItem.CheckMenuItemAppearance" />
	</replace-with>

	<!-- Status -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyStatusAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.Status.StatusAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyBoxStatusAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.Status.BoxStatusAppearance" />
	</replace-with>

	<!-- Tab Panel -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyTabPanelAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.TabPanel.TabPanelAppearance" />
