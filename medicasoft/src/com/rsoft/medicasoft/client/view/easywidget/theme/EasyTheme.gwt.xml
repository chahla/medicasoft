<module>
	<inherits name="com.sencha.gxt.theme.base.Base" />
	<replace-with
		class="com.sencha.gxt.theme.gray.client.toolbar.GrayToolBarButtonAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.toolbar.ToolBarButton.ToolBarButtonAppearance" />
	</replace-with>

	<replace-with
		class="com.sencha.gxt.theme.gray.client.panel.GrayPanelHeaderAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.ContentPanel.PanelHeaderAppearance" />
	</replace-with>

 
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyThemeAppearance">
		<when-type-is
			class="com.sencha.gxt.core.client.resources.ThemeStyles.ThemeAppearance" />
	</replace-with>

	<!-- Buttons -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyButtonGroupAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.button.ButtonGroup.ButtonGroupAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyGridAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.grid.GridView.GridAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyProgressBarAppearance">
		<when-type-is
			class="com.sencha.gxt.cell.core.client.ProgressBarCell.ProgressBarAppearance" />
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyWindowAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.Window.WindowAppearance" /> <!-- Need Framed panel -->
	</replace-with>

	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyAccordionLayoutAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.container.AccordionLayoutContainer.AccordionLayoutAppearance" />
	</replace-with>


	<!-- Menu Bar -->
	<replace-with
		class="com.rsoft.medicasoft.client.view.easywidget.appearance.EasyMenuBarAppearance">
		<when-type-is
			class="com.sencha.gxt.widget.core.client.menu.MenuBar.MenuBarAppearance" />
	</replace-with>

